{
  "name": "typescript-action",
  "version": "1.1.2",
  "private": true,
  "description": "get .NET SDK version project from csproj/vbproj.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --source-map --license licenses.txt",
    "buildtest": "node __tests__/makeTest.js",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run buildtest && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kzrnm/get-net-sdk-project-versions-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "kzrnm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@xmldom/xmldom": "^0.8.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^15.0.1",
    "@types/istanbul-lib-report": "^3.0.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.8.0",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "js-yaml": "^4.1.0",
    "node-notifier": ">=10.0.1",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
