{
  "name": "typescript-action",
  "version": "3.0.0",
  "private": true,
  "description": "get .NET SDK version project from csproj/vbproj.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --source-map --license licenses.txt",
    "buildtest": "node __tests__/makeTest.js",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run buildtest && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kzrnm/get-net-sdk-project-versions-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "kzrnm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@xmldom/xmldom": "^0.8.6"
  },
  "devDependencies": {
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "node-notifier": ">=10.0.1",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
