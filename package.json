{
  "name": "typescript-action",
  "version": "1.0.0",
  "private": true,
  "description": "get .NET SDK version project from csproj/vbproj.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --source-map --license licenses.txt",
    "buildtest": "node __tests__/makeTest.js",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run buildtest && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "naminodarie",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "xmldom": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.10",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/parser": "^4.16.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.14.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.3.5",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.1.2"
  }
}
